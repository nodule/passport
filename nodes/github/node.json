{
  "name": "github",
  "ns": "passport",
  "description": "Github Strategy",
  "keywords": ["github", "strategy"],
  "require": {
    "passport-github": "0.x.x"
  },
  "phrases": {
    "active": "Creating Github Strategy"
  },
  "ports": {
    "input": {
      "title": "Passport",
      "passport": {
        "type": "function"
      },
      "credentials": {
         "title": "Credentials",
         "type": "object",
         "properties": {
           "clientID": {
             "title": "Client ID",
             "type": "string"
           },
           "clientSecret": {
             "title": "Client Secret",
             "type": "password"
           }
         }
      
      },
      "callbackURL": {
        "title": "Callback URL",
        "type": "uri"
      }
    },
    "output": {
      "strategy": {
        "title": "Strategy",
        "type": "object"
      }
    }
  }
}
