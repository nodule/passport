{
  "name": "facebook",
  "ns": "passport",
  "title": "Facebook Strategy",
  "description": "Facebook Strategy",
  "keywords": [
    "facebook",
    "strategy"
  ],
  "phrases": {
    "active": "Creating Facebook Strategy"
  },
  "ports": {
    "input": {
      "passport": {
        "title": "Passport",
        "type": "Authenticator"
      },
      "credentials": {
        "title": "Credentials",
        "type": "object",
        "properties": {
          "clientID": {
            "title": "Facebook App Id",
            "type": "string"
          },
          "clientSecret": {
            "title": "Facebook App Secret",
            "type": "password"
          }
        }
      },
      "callbackURL": {
        "title": "Callback URL",
        "type": "url"
      }
    },
    "output": {
      "strategy": {
        "title": "Strategy",
        "type": "Strategy"
      }
    }
  },
  "dependencies": {
    "npm": {
      "passport-facebook": "1.x.x"
    }
  }
}