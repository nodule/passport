{
  "name": "passport",
  "description": "Passport components for ChiÏ‡",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodule/passport.git"
  },
  "dependencies": {
    "passport": "0.x.x",
    "passport-local": "0.x.x",
    "passport-facebook": "1.x.x",
    "passport-twitter": "1.x.x",
    "passport-github": "0.x.x"
  },
  "nodes": [
    {
      "name": "authenticate",
      "ns": "passport",
      "description": "Passport Authenticate",
      "keywords": [
        "passport"
      ],
      "phrases": {
        "active": "Authenticating to {{input.strategy.name}}"
      },
      "ports": {
        "input": {
          "title": "Passport",
          "passport": {
            "type": "function"
          },
          "strategy": {
            "title": "Strategy",
            "description": "Note: will use the .name property of the strategy",
            "type": "object"
          },
          "options": {
            "title": "Options",
            "type": "object",
            "properties": {
              "failureRedirect": {
                "title": "Failure Redirect",
                "type": "string",
                "description": "e.g. /login"
              }
            }
          }
        },
        "output": {
          "req": {
            "title": "Request",
            "type": "object"
          },
          "res": {
            "title": "Response",
            "type": "object"
          }
        }
      },
      "env": "server",
      "fn": "output = [passport, 'authenticate', input.strategy.name, input.options]\n"
    },
    {
      "name": "deserializeUser",
      "ns": "passport",
      "description": "Passport deserializeUser",
      "keywords": [
        "passport"
      ],
      "phrases": {
        "active": "Deserializing user"
      },
      "ports": {
        "input": {
          "title": "Passport",
          "passport": {
            "type": "function"
          }
        },
        "output": {
          "user": {
            "title": "id",
            "type": "string"
          },
          "done": {
            "title": "Done",
            "type": "function"
          }
        }
      },
      "env": "server",
      "fn": "output = [passport, 'deserializeUser']\n"
    },
    {
      "name": "facebook",
      "ns": "passport",
      "description": "Facebook Strategy",
      "keywords": [
        "facebook",
        "strategy"
      ],
      "require": {
        "passport-facebook": "1.x.x"
      },
      "phrases": {
        "active": "Creating Facebook Strategy"
      },
      "ports": {
        "input": {
          "title": "Passport",
          "passport": {
            "type": "function"
          },
          "credentials": {
            "title": "Credentials",
            "type": "object",
            "properties": {
              "clientID": {
                "title": "Facebook App Id",
                "type": "string"
              },
              "clientSecret": {
                "title": "Facebook App Secret",
                "type": "password"
              }
            }
          },
          "callbackURL": {
            "title": "Callback URL",
            "type": "uri"
          }
        },
        "output": {
          "strategy": {
            "title": "Strategy",
            "type": "object"
          }
        }
      },
      "env": "server",
      "fn": "output = {\n  strategy: new passportFacebook.Strategy( {\n    clientID: input.credentials.clientID,\n    clientSecret: input.credentials.clientSecret,\n    callbackURL: input.callbackURL\n  })\n};\n"
    },
    {
      "name": "github",
      "ns": "passport",
      "description": "Github Strategy",
      "keywords": [
        "github",
        "strategy"
      ],
      "require": {
        "passport-github": "0.x.x"
      },
      "phrases": {
        "active": "Creating Github Strategy"
      },
      "ports": {
        "input": {
          "title": "Passport",
          "passport": {
            "type": "function"
          },
          "credentials": {
            "title": "Credentials",
            "type": "object",
            "properties": {
              "clientID": {
                "title": "Client ID",
                "type": "string"
              },
              "clientSecret": {
                "title": "Client Secret",
                "type": "password"
              }
            }
          },
          "callbackURL": {
            "title": "Callback URL",
            "type": "uri"
          }
        },
        "output": {
          "strategy": {
            "title": "Strategy",
            "type": "object"
          }
        }
      },
      "env": "server",
      "fn": "output = {\n  strategy: new passportGithub.Strategy( {\n    clientID: input.credentials.clientID,\n    clientSecret: input.credentials.clientSecret,\n    callbackURL: input.callbackURL\n  })\n};\n"
    },
    {
      "name": "initialize",
      "ns": "passport",
      "description": "Passport Initialize",
      "depends": [
        "passport:initialize"
      ],
      "phrases": {
        "active": "Initializing passport"
      },
      "ports": {
        "input": {
          "title": "Passport",
          "passport": {
            "type": "function"
          },
          "app": {
            "title": "Express App",
            "type": "function"
          }
        },
        "output": {
          "event:complete": {
            "title": "Complete",
            "type": "object"
          }
        }
      },
      "env": "server",
      "fn": "output = [input.app, 'use', input.passport.initialize()]\n"
    },
    {
      "name": "passport",
      "ns": "passport",
      "description": "Require Passport",
      "keywords": [
        "passport"
      ],
      "require": {
        "passport": "0.x.x"
      },
      "phrases": {
        "active": "Requiring passport"
      },
      "ports": {
        "input": {},
        "output": {
          "passport": {
            "title": "Passport",
            "type": "function"
          }
        }
      },
      "env": "server",
      "fn": "output = { passport: passport }\n"
    },
    {
      "name": "serializeUser",
      "ns": "passport",
      "description": "Passport serializeUser",
      "keywords": [
        "passport"
      ],
      "phrases": {
        "active": "Serialized user"
      },
      "ports": {
        "input": {
          "title": "Passport",
          "passport": {
            "type": "function"
          }
        },
        "output": {
          "user": {
            "title": "User",
            "type": "object"
          },
          "done": {
            "title": "Done",
            "type": "function"
          }
        }
      },
      "env": "server",
      "fn": "output = [passport, 'serializeUser']\n"
    },
    {
      "name": "session",
      "ns": "passport",
      "description": "Passport session",
      "depends": [
        "passport:session"
      ],
      "phrases": {
        "active": "Creating Passport session"
      },
      "ports": {
        "input": {
          "title": "Passport",
          "passport": {
            "type": "function"
          },
          "app": {
            "title": "Express App",
            "type": "function"
          }
        },
        "output": {
          "event:complete": {
            "title": "Complete",
            "type": "object"
          }
        }
      },
      "env": "server",
      "fn": "output = [input.app, 'use', input.passport.session()]\n"
    },
    {
      "name": "twitter",
      "ns": "passport",
      "description": "Twitter Strategy",
      "keywords": [
        "twitter",
        "strategy"
      ],
      "require": {
        "passport-twitter": "1.x.x"
      },
      "phrases": {
        "active": "Creating Twitter Strategy"
      },
      "ports": {
        "input": {
          "title": "Passport",
          "passport": {
            "type": "function"
          },
          "credentials": {
            "title": "Credentials",
            "type": "object",
            "properties": {
              "consumerKey": {
                "title": "Twitter Consumer Key",
                "type": "string"
              },
              "consumerSecret": {
                "title": "Twitter Consumer Secret",
                "type": "password"
              }
            }
          },
          "callbackURL": {
            "title": "Callback URL",
            "type": "uri"
          }
        },
        "output": {
          "strategy": {
            "title": "Strategy",
            "type": "object"
          }
        }
      },
      "env": "server",
      "fn": "output = {\n  strategy: new passportTwitter.Strategy( {\n    consumerKey: input.credentials.consumerKey,\n    consumerSecret: input.credentials.consumerSecret,\n    callbackURL: input.callbackURL\n  })\n};\n"
    },
    {
      "name": "use",
      "ns": "passport",
      "description": "Passport Use",
      "keywords": [
        "passport",
        "use"
      ],
      "phrases": {
        "active": "Using passport strategy {{input.strategy.name}}"
      },
      "ports": {
        "input": {
          "title": "Passport",
          "passport": {
            "type": "function"
          },
          "strategy": {
            "title": "Strategy",
            "type": "object"
          }
        },
        "output": {
          "accessToken": {
            "title": "Access Token",
            "type": "string"
          },
          "refreshToken": {
            "title": "Refresh Token",
            "type": "string"
          },
          "profile": {
            "title": "ProfileToken",
            "type": "object"
          },
          "done": {
            "title": "Done",
            "type": "function"
          }
        }
      },
      "env": "server",
      "fn": "output = [passport, 'use', input.strategy]\n"
    }
  ],
  "twigs": [],
  "env": "server"
}